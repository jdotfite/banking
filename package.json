    "lucide-react": "^0.503.0",
    "next": "^14.2.3",
    "next-pwa": "^5.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-spring": "^9.5.5",
    "recharts": "^2.15.3",
    "swr": "^2.3.3",
    "workbox-window": "^7.0.0"
  },
